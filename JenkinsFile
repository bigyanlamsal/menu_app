pipeline {
    agent any
    options {
        // Timeout counter starts AFTER agent is allocated
        timeout(time: 1, unit: 'SECONDS')
    }
    stages {
        stage('Build') {
            steps {
                echo 'Building'

               // sh 'npm command for js' --like as
            }
        }
        
       stage('Test') {
            steps {
                echo 'Hello World'
            }
        }
    }
}
